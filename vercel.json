{
    "rewrites": [
        {
            "source": "/assets/(.*)",
            "destination": "/assets/$1"
        },
        {
            "source": "/sw.js",
            "destination": "/sw.js"
        },
        {
            "source": "/manifest.json",
            "destination": "/manifest.json"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}
